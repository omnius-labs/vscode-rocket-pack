{
  "name": "Rocket Pack 1",
  "scopeName": "source.rocket-pack",
  "fileTypes": [
    "rocket-pack"
  ],
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#syntax"
    },
    {
      "include": "#namespace"
    },
    {
      "include": "#using"
    },
    {
      "include": "#option"
    },
    {
      "include": "#attribute"
    },
    {
      "include": "#enum"
    },
    {
      "include": "#struct"
    },
    {
      "include": "#table"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.block.rocket-pack",
          "begin": "/\\*",
          "end": "\\*/"
        },
        {
          "name": "comment.line.double-slash.rocket-pack",
          "begin": "//",
          "end": "$\\n?"
        }
      ]
    },
    "syntax": {
      "match": "\\b(syntax)\\s+(v[\\d|\\.]+)\\s*;",
      "captures": {
        "1": {
          "name": "constant.language.rocket-pack"
        },
        "2": {
          "name": "string.unquoted.rocket-pack.syntax"
        }
      }
    },
    "namespace": {
      "match": "\\b(namespace)\\s+(\"[^\"]+\")\\s*;",
      "captures": {
        "1": {
          "name": "constant.language.rocket-pack"
        },
        "2": {
          "name": "string.quoted.double.rocket-pack.namespace"
        }
      }
    },
    "using": {
      "match": "\\b(using)\\s+(\"[^\"]+\")\\s*;",
      "captures": {
        "1": {
          "name": "constant.language.rocket-pack"
        },
        "2": {
          "name": "string.quoted.double.rocket-pack.using"
        }
      }
    },
    "option": {
      "match": "\\b(option)\\s+(\\w+)\\s*(\"[^\"]+\")\\s*;",
      "captures": {
        "1": {
          "name": "constant.language.rocket-pack"
        },
        "2": {
          "name": "constant.language.rocket-pack"
        },
        "3": {
          "name": "string.quoted.double.rocket-pack.option"
        }
      }
    },
    "attribute": {
      "match": "\\[\\s*(\\w+)\\s*\\]",
      "captures": {
        "1": {
          "name": "keyword.other.rocket-pack"
        }
      }
    },
    "enum": {
      "begin": "(?=\\benum\\b)",
      "end": "(?<=\\})",
      "patterns": [
        {
          "begin": "(?x)\n\\b(enum)\\b\\s+\n([A-Za-z][A-Za-z0-9_]*)\\s*:\\s*(\\w+)\\s*",
          "beginCaptures": {
            "1": {
              "name": "storage.type.enum.rocket-pack"
            },
            "2": {
              "name": "entity.name.type.enum.rocket-pack"
            },
            "3": {
              "patterns": [
                {
                  "include": "#enumType"
                }
              ]
            }
          },
          "end": "(?=\\{)"
        },
        {
          "patterns": [
            {
              "include": "#enumElement"
            }
          ]
        }
      ]
    },
    "enumElement": {
      "match": "\\b([A-Za-z0-9_]+)\\s*=\\s*(\\d+)\\s*,\\s*",
      "captures": {
        "1": {
          "name": "variable.parameter.rocket-pack"
        },
        "2": {
          "name": "constant.numeric.rocket-pack"
        }
      }
    },
    "table": {
      "begin": "(?=\\btable\\b)",
      "end": "(?<=\\})",
      "patterns": [
        {
          "begin": "(?x)\n\\b(table)\\b\\s+\n([A-Za-z][A-Za-z0-9_]*)\\s*",
          "beginCaptures": {
            "1": {
              "name": "storage.type.table.rocket-pack"
            },
            "2": {
              "name": "entry.name.type.rocket-pack"
            },
            "3": {
              "patterns": [
                {
                  "include": "#tableType"
                }
              ]
            }
          },
          "end": "(?=\\{)"
        },
        {
          "patterns": [
            {
              "include": "#tableElement"
            }
          ]
        }
      ]
    },
    "tableElement": {
      "match": "\\s*([A-Za-z0-9_]+)\\s*:\\s*(.+)\\s*=\\s*(\\d+)\\s*,\\s*",
      "captures": {
        "1": {
          "name": "variable.parameter.rocket-pack"
        },
        "2": {
          "patterns": [
            {
              "include": "#comment"
            },
            {
              "include": "#standardType"
            },
            {
              "include": "#bytesType"
            },
            {
              "include": "#stringType"
            },
            {
              "include": "#vectorType"
            },
            {
              "include": "#mapType"
            },
            {
              "include": "#customType"
            }
          ]
        },
        "3": {
          "name": "constant.numeric.rocket-pack"
        }
      }
    },
    "struct": {
      "begin": "(?=\\bstruct\\b)",
      "end": "(?<=\\})",
      "patterns": [
        {
          "begin": "(?x)\n\\b(struct)\\b\\s+\n([A-Za-z][A-Za-z0-9_]*)\\s*",
          "beginCaptures": {
            "1": {
              "name": "storage.type.struct.rocket-pack"
            },
            "2": {
              "name": "entry.name.type.rocket-pack"
            },
            "3": {
              "patterns": [
                {
                  "include": "#structType"
                }
              ]
            }
          },
          "end": "(?=\\{)"
        },
        {
          "patterns": [
            {
              "include": "#structElement"
            }
          ]
        }
      ]
    },
    "structElement": {
      "match": "\\s*([A-Za-z0-9_]+)\\s*:\\s*(.+)\\s*,\\s*",
      "captures": {
        "1": {
          "name": "variable.parameter.rocket-pack"
        },
        "2": {
          "patterns": [
            {
              "include": "#comment"
            },
            {
              "include": "#standardType"
            },
            {
              "include": "#bytesType"
            },
            {
              "include": "#stringType"
            },
            {
              "include": "#vectorType"
            },
            {
              "include": "#mapType"
            },
            {
              "include": "#customType"
            }
          ]
        }
      }
    },
    "enumType": {
      "match": "\\s*(int8|int16|int32|int64|uint8|uint16|uint32|uint64)\\??\\s*",
      "captures": {
        "1": {
          "name": "storage.type.rocket-pack"
        }
      }
    },
    "standardType": {
      "match": "\\s*(bool|int8|int16|int32|int64|uint8|uint16|uint32|uint64|float32|float64|timestamp)\\??\\s*",
      "captures": {
        "1": {
          "name": "storage.type.rocket-pack"
        }
      }
    },
    "bytesType": {
      "match": "\\s*(bytes)\\??\\s*(\\((.*?)\\))?\\s*",
      "captures": {
        "1": {
          "name": "storage.type.rocket-pack"
        },
        "3": {
          "patterns": [
            {
              "include": "#typeParam"
            },
            {
              "include": "#separator"
            }
          ]
        }
      }
    },
    "stringType": {
      "match": "\\s*(string)\\??\\s*(\\((.*?)\\))?\\s*",
      "captures": {
        "1": {
          "name": "storage.type.rocket-pack"
        },
        "3": {
          "patterns": [
            {
              "include": "#typeParam"
            },
            {
              "include": "#separator"
            }
          ]
        }
      }
    },
    "vectorType": {
      "match": "\\s*(vector)\\s*<\\s*([^>]+)\\s*>\\??\\s*(\\((.*?)\\))?\\s*",
      "captures": {
        "1": {
          "name": "storage.type.rocket-pack"
        },
        "2": {
          "patterns": [
            {
              "include": "#standardType"
            },
            {
              "include": "#bytesType"
            },
            {
              "include": "#stringType"
            },
            {
              "include": "#customType"
            }
          ]
        },
        "4": {
          "patterns": [
            {
              "include": "#typeParam"
            },
            {
              "include": "#separator"
            }
          ]
        }
      }
    },
    "mapType": {
      "match": "\\s*(map)\\s*<\\s*([^>]+)\\s*,\\s*([^>]+)\\s*>\\??\\s*(\\((.*?)\\))?\\s*",
      "captures": {
        "1": {
          "name": "storage.type.rocket-pack"
        },
        "2": {
          "patterns": [
            {
              "include": "#standardType"
            },
            {
              "include": "#bytesType"
            },
            {
              "include": "#stringType"
            },
            {
              "include": "#customType"
            }
          ]
        },
        "3": {
          "patterns": [
            {
              "include": "#standardType"
            },
            {
              "include": "#bytesType"
            },
            {
              "include": "#stringType"
            },
            {
              "include": "#customType"
            }
          ]
        },
        "5": {
          "patterns": [
            {
              "include": "#typeParam"
            },
            {
              "include": "#separator"
            }
          ]
        }
      }
    },
    "customType": {
      "match": "\\s*([A-Za-z0-9_]+)\\??\\s*",
      "captures": {
        "1": {
          "name": "support.class.rocket-pack"
        }
      }
    },
    "typeParam": {
      "match": "\\s*([A-Za-z0-9_]+)\\s*:\\s*([A-Za-z0-9_]+)\\s*",
      "captures": {
        "1": {
          "name": "variable.rocket-pack"
        },
        "2": {
          "name": "constant.rocket-pack"
        }
      }
    },
    "string": {
      "match": "('([^']|\\')*')|(\"([^\"]|\\\")*\")",
      "name": "string.quoted.double.rocket-pack"
    },
    "separator": {
      "match": ",",
      "name": "punctuation.separator.parameter.rocket-pack"
    }
  }
}
